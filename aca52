#include <iostream>

class Matrix {
private:
    int data[3][3];

public:
    void input() {
        std::cout << "Mutqagreq 3x3 matric\n";
        for (int i = 0; i < 3; i++) {
            for (int j = 0; j < 3; j++) {
                std::cin >> data[i][j];
            }
        }
    }

    void display() const {
        for (int i = 0; i < 3; i++) {
            for (int j = 0; j < 3; j++) {
                std::cout << data[i][j] << " ";
            }
            std::cout << std::endl;
        }
    }

    
    Matrix operator++() {
        for (int i = 0; i < 3; i++) {
            for (int j = 0; j < 3; j++) {
                data[i][j]++; 
            }
        }
        return *this; 
    }
};

int main() {
    Matrix A;

    std::cout << "Mutqagreq matric A:\n";
    A.input();

    ++A; 

    std::cout << "A++-ic heto matric-y:\n";
    A.display();

    return 0;
}
