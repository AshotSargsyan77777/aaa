#include <iostream>

class Matrix {
private:
    int data[3][3];

public:
    void input() {
        std::cout << "Mutqagreq 3x3 matric\n";
        for (int i = 0; i < 3; i++) {
            for (int j = 0; j < 3; j++) {
                std::cin >> data[i][j];
            }
        }
    }

    void display() const {
        for (int i = 0; i < 3; i++) {
            for (int j = 0; j < 3; j++) {
                std::cout << data[i][j] << " ";
            }
            std::cout << std::endl;
        }
    }

    
    Matrix operator*(int num) const {
        Matrix result;
        for (int i = 0; i < 3; i++) {
            for (int j = 0; j < 3; j++) {
                result.data[i][j] = data[i][j] * num;
            }
        }
        return result;
    }
};

int main() {
    Matrix A, C;
    int num;

    std::cout << "Mutqagreq matric A\n";
    A.input();

    std::cout << "Mutqagreq tiv (orinak 5)\n";
    std::cin >> num;

    C = A * num;   

    std::cout << "A * " << num << " = \n";
    C.display();

    return 0;
}
